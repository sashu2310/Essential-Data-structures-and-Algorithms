//upper bound algorithm is used to find the first element which is strictly greater than the given element

#include<stdio.h>
int upperBound(int n,int key,int *A) {
    int start=0,end=n-1,mid,ans=-1;
    while(start < end) {
        mid = start+(end-start)/2;
//got the element, since elements on the right half will be surely greater than the given element so lets move to left half to find
        if(A[mid]>key) {
            ans = A[mid];
            end = mid-1;
        }
 //key is small check the right half
        else if(A[mid]<=key)
            start = mid+1;
    }
return ans;
}
int main() {
    int A[5] = {2,3,3,6,8};
    printf("%d",upperBound(5,3,&A[0]));
    return 0;
}
